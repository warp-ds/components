{
  "name": "@warp-ds/components",
  "version": "1.0.0-beta.1",
  "description": "Next Gen Warp Components",
  "type": "module",
  "exports": {
    ".": {
      "types": "./types/jsx-integration.d.ts"
    },
    "./jsx": {
      "types": "./types/jsx-integration.d.ts"
    },
    "./*": {
      "types": "./dist/packages/*/index.d.ts",
      "default": "./dist/packages/*/index.js"
    },
    "./react/*": {
      "types": "./dist/react/*/index.d.ts",
      "default": "./dist/react/*/index.js"
    },
    "./custom-elements.json": "./dist/custom-elements.json"
  },
  "typesVersions": {
    "*": {
      "./*": [
        "./dist/packages/*/index.d.ts"
      ],
      "./react/*": [
        "./dist/react/*/index.d.ts"
      ]
    }
  },
  "types": "./types/jsx-integration.d.ts",
  "files": [
    "dist/",
    "./types"
  ],
  "scripts": {
    "build-storybook:lit": "storybook build -c .storybook-lit -o storybook/lit",
    "build-storybook:react": "storybook build -c .storybook-react -o storybook/react",
    "build-storybook": "pnpm run build-storybook:react && pnpm run build-storybook:lit",
    "build:manifest": "cem analyze --config cem/config.js",
    "build": "rm -rf dist && mkdir dist && node build.js && npm run build:manifest",
    "check": "pnpm biome check --write .",
    "format": "pnpm biome format --write .",
    "lint:check": "pnpm biome lint .",
    "lint": "pnpm biome lint --write .",
    "messages:extract": "lingui extract",
    "messages:compile": "lingui compile",
    "start:watch": "web-dev-server --open / --node-resolve --watch",
    "start": "web-dev-server --open / --node-resolve",
    "storybook:lit": "storybook dev -p 6007 -c .storybook-lit",
    "storybook:react": "storybook dev -p 6006 -c .storybook-react",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@biomejs/biome": "2.0.0",
    "@chromatic-com/storybook": "^4.1.0",
    "@custom-elements-manifest/analyzer": "^0.10.5",
    "@lingui/cli": "5.2.0",
    "@lingui/conf": "5.2.0",
    "@lingui/core": "5.2.0",
    "@storybook/addon-a11y": "9.1.1",
    "@storybook/addon-docs": "9.1.1",
    "@storybook/addon-onboarding": "9.1.1",
    "@storybook/react-vite": "9.1.1",
    "@storybook/web-components-vite": "9.1.1",
    "@types/node": "^24.0.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@warp-ds/icons": "^2.5.0",
    "@warp-ds/react": "^2.1.0",
    "@wc-toolkit/jsx-types": "^1.3.0",
    "@web/dev-server": "^0.4.3",
    "classnames": "^2.5.1",
    "custom-element-react-wrappers": "^1.7.2",
    "date-fns": "^4.1.0",
    "esbuild": "^0.20.1",
    "esbuild-plugin-inline-import": "^1.1.0",
    "fs-extra": "^11.3.0",
    "glob": "^10.3.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup-plugin-inline-code": "^1.2.7",
    "storybook": "9.1.1",
    "ts-lit-plugin": "^2.0.2",
    "typescript": "^5.7.3",
    "use-effect-reducer": "^0.7.0",
    "vite": "^6.1.0"
  },
  "peerDependencies": {
    "@lingui/core": "5.x",
    "@lit/react": "^1.0.3",
    "lit": "^3.1.2"
  },
  "customElements": "dist/custom-elements.json"
}
